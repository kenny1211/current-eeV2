<div id="home">
  <div class="background">
    <div class="row">
      <div class="col-12">
      </div>
    </div>
    <br>
    <br>
    <h1 class="text-center text-light">Current$ee Intro Questionaire</h1>
    <br>
    <br>
  </div>

<div class="container">

  <div class="input-group mb-3">
    <div class="input-group-prepend">
      <span class="input-group-text">Income</span>
    </div>
    <input data-name="income" id="income" type="text" class="form-control">
  </div>

  <div class="input-group mb-3">
    <div class="input-group-prepend">
      <span class="input-group-text">Savings</span>
    </div>
    <input data-name="savings" id="savings" type="text" class="form-control">
  </div>

  <div class="input-group mb-3">
    <div class="input-group-prepend">
      <span class="input-group-text" >Travel Expense</span>
    </div>
    <input data-name="travel" id="travel" type="text" class="form-control">
  </div>

  <div class="input-group mb-3">
    <div class="input-group-prepend">
      <span class="input-group-text">Rent/Mortgage</span>
    </div>
    <input data-name="home" id="home" type="text" class="form-control">
  </div>

  <div class="input-group mb-3">
    <div class="input-group-prepend">
      <span class="input-group-text">Utilities</span>
    </div>
    <input data-name="utilities" id="utilities" type="text" class="form-control">
  </div>

  <div class="input-group mb-3">
    <div class="input-group-prepend">
      <span class="input-group-text">Health/Fitness</span>
    </div>
    <input data-name="health-fitness" id="health-fitness" type="text" class="form-control">
  </div>

  <div class="input-group mb-3">
    <div class="input-group-prepend">
      <span class="input-group-text">Date</span>
    </div>
    <input id="date" type="text" class="form-control">
  </div>

  <div class="input-group">
    <button type="button submit" class="btn btn-primary btn-lg btn-block" id="introquestSubmit">
      Submit Basic Budget Information
    </button>
  </div>

</div>
